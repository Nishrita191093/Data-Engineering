{
	"name": "df_task12",
	"properties": {
		"folder": {
			"name": "df_Nishrita/df_tasks"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "task12_customerssrc1",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "task12_customerssrc2",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "task12_outputcustomers",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "union1"
				},
				{
					"name": "window1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          customer_id as string,",
				"          name as string,",
				"          email as string,",
				"          age as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          customer_id as string,",
				"          name as string,",
				"          email as string,",
				"          age as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source2",
				"source1, source2 union(byName: true)~> union1",
				"union1 window(over(customer_id),",
				"     desc(customer_id, true),",
				"     rank = rowNumber()) ~> window1",
				"window1 filter(rank==1) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          customer_id as integer,",
				"          name as string,",
				"          email as string,",
				"          age as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          customer_id,",
				"          name,",
				"          email,",
				"          age",
				"     )) ~> sink1"
			]
		}
	}
}