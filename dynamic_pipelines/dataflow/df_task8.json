{
	"name": "df_task8",
	"properties": {
		"folder": {
			"name": "df_Nishrita/df_tasks"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "task8_srcusers",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "task8_tgtusers",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "sort1"
				},
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          user_id as short,",
				"          timestamp as timestamp,",
				"          name as string,",
				"          email as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sort(desc(timestamp, true)) ~> sort1",
				"sort1 aggregate(groupBy(user_id,",
				"          name),",
				"     timestamp = first(timestamp)) ~> aggregate1",
				"aggregate1 sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     input(",
				"          user_id as integer,",
				"          name as string,",
				"          timestamp as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          user_id,",
				"          name,",
				"          timestamp",
				"     )) ~> sink1"
			]
		}
	}
}